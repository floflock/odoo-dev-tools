{
  "name": "odoo-dev-tools",
  "version": "1.1.0",
  "description": "Chrome extension with developer tools for Odoo - Debug mode, quick links, reports, and more",
  "type": "module",
  "scripts": {
    "dev": "wxt",
    "dev:firefox": "wxt -b firefox",
    "build": "wxt build",
    "build:firefox": "wxt build -b firefox",
    "zip": "wxt build && rm -rf .output/chrome-mv3/screenshots .output/chrome-mv3/ICONS.md && cd .output/chrome-mv3 && zip -r ../odoo-dev-tools-1.1.0-chrome.zip . && cd ../..",
    "zip:firefox": "wxt zip -b firefox",
    "postinstall": "wxt prepare"
  },
  "keywords": [
    "odoo",
    "chrome-extension",
    "developer-tools",
    "debug"
  ],
  "author": "",
  "license": "MIT",
  "packageManager": "pnpm@10.27.0",
  "devDependencies": {
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@wxt-dev/module-react": "^1.1.5",
    "sass": "^1.97.2",
    "typescript": "^5.9.3",
    "wxt": "^0.20.13"
  },
  "dependencies": {
    "react": "^19.2.3",
    "react-dom": "^19.2.3"
  },
  "pnpm": {
    "overrides": {
      "vite": "^6.0.0"
    }
  }
}
